# Remove fish default greeting
set --erase fish_greeting

# Path to your oh-my-fish.
set fish_path $HOME/.oh-my-fish

# Plugins
set fish_plugins brew boxen boot2docker bundler cliniko dpaste extract ndenv osx rails z rbenv

# Theme
set fish_theme l

# Path to your custom folder.
set fish_custom $HOME/Dropbox/Documents/Bruno/dotfiles/oh-my-fish

# Load oh-my-fish cofiguration.
. $fish_path/oh-my-fish.fish
